{
  "name": "ui-web",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn watch:css & vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "yarn watch:css & start-storybook -p 3011",
    "build-storybook": "yarn build:css && build-storybook",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:nofix": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "build:css": "cross-env NODE_ENV='production' postcss static/style.css -o src/assets/css/style.css",
    "build:css:dev": "cross-env postcss static/style.css -o src/assets/css/style.css",
    "watch:css": "cross-env NODE_ENV='production' postcss static/style.css -o src/assets/css/style.css --watch",
    "watch:css:dev": "cross-env NODE_ENV='development' postcss static/style.css -o src/assets/css/style.css --watch",
    "test": "jest ./src",
    "test:watch": "jest ./src --watch",
    "test:playwright": "playwright test",
    "py:open": "playwright open",
    "py:codegen": "playwright codegen"
  },
  "dependencies": {
    "@xstate/react": "^3.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "xstate": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@mdx-js/react": "^2.1.1",
    "@playwright/test": "^1.22.2",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-docs": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@tailwindcss/ui": "^0.7.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.10",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^28.0.4",
    "twrnc": "^3.3.2",
    "typescript": "^4.7.2",
    "vite": "^2.9.9"
  }
}
