{
  "name": "with-context",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "prettier": "^2.6.2",
    "turbo": "^1.2.14"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=17.0.0"
  },
  "packageManager": "yarn@3.2.1"
}
